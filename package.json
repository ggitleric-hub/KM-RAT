{
  "name": "grob-rat-rat",
  "version": "1.0.0",
  "private": true,
  "main": "electron/main.js",
  "description": "Grob Rat RAT - Remote Administration Tool",
  "author": "Grob Rat",
  "scripts": {
    "dev": "cross-env NODE_ENV=development electron .",
    "dev:next": "next dev",
    "build": "next build",
    "build:electron": "npm run build && electron-builder",
    "build:electron:win": "npm run build && electron-builder --win --x64 --ia32",
    "build:electron:dir": "npm run build && electron-builder --win --dir",
    "start": "cross-env NODE_ENV=production electron .",
    "lint": "eslint ."
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "1.1.1",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-context-menu": "2.2.4",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-hover-card": "1.1.4",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-menubar": "1.1.4",
    "@radix-ui/react-navigation-menu": "1.2.3",
    "@radix-ui/react-popover": "1.1.6",
    "@radix-ui/react-progress": "1.1.1",
    "@radix-ui/react-radio-group": "1.2.2",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slider": "1.2.2",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/react-tabs": "1.1.2",
    "@radix-ui/react-toast": "1.2.4",
    "@radix-ui/react-toggle": "1.1.1",
    "@radix-ui/react-toggle-group": "1.1.1",
    "@radix-ui/react-tooltip": "1.1.6",
    "@vercel/analytics": "1.3.1",
    "autoprefixer": "^10.4.20",
    "better-sqlite3": "12.5.0",
    "bufferutil": "latest",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "date-fns": "4.1.0",
    "embla-carousel-react": "8.5.1",
    "input-otp": "1.4.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.454.0",
    "next": "16.0.10",
    "next-themes": "^0.4.6",
    "react": "19.2.0",
    "react-day-picker": "9.8.0",
    "react-dom": "19.2.0",
    "react-hook-form": "7.60.0",
    "react-resizable-panels": "2.1.7",
    "recharts": "2.15.4",
    "sonner": "1.7.4",
    "tailwind-merge": "3.3.1",
    "tailwindcss-animate": "1.0.7",
    "utf-8-validate": "latest",
    "uuid": "13.0.0",
    "vaul": "1.1.2",
    "websockets": "0.2.0",
    "ws": "8.19.0",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "electron": "36.0.0",
    "@electron/notarize": "^1.2.4",
    "@tailwindcss/postcss": "^4.1.9",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "electron-builder": "^25.1.8",
    "postcss": "^8.5",
    "tailwindcss": "^4.1.9",
    "tw-animate-css": "1.3.3",
    "typescript": "^5"
  },
  "build": {
    "appId": "com.grobrat.rat",
    "productName": "Grob Rat RAT",
    "copyright": "Copyright Â© 2024 Grob Rat",
    "directories": {
      "output": "release",
      "buildResources": "electron/resources"
    },
    "files": [
      {
        "from": ".",
        "filter": [
          "!node_modules/**",
          "!electron/**",
          "!out/**",
          "!release/**",
          "!.git/**",
          "!.next/**",
          "!public/**",
          "!scripts/**",
          "!server/**",
          "!styles/**",
          "!components/**",
          "!app/**",
          "!lib/**",
          "!hooks/**",
          "!client/**"
        ]
      },
      {
        "from": ".next/server",
        "to": ".next/server"
      },
      {
        "from": ".next/static",
        "to": ".next/static"
      },
      {
        "from": "public",
        "to": "public"
      }
    ],
    "extraResources": [
      {
        "from": "server",
        "to": "server"
      }
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": ["x64", "ia32"]
        },
        {
          "target": "zip",
          "arch": ["x64", "ia32"]
        }
      ],
      "icon": "public/icon.ico",
      "artifactName": "GrobRat-${version}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Grob Rat RAT"
    },
    "linux": {
      "target": ["AppImage", "deb"],
      "icon": "public/icon.png"
    }
  }
}
